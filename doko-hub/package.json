{
  "name": "doko-hub",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync; npm run smui-prepare",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:silent": "cross-env node -r dotenv/config node_modules/jest/bin/jest.js --runInBand --silent",
    "test": "cross-env node -r dotenv/config node_modules/jest/bin/jest.js --runInBand",
    "db:start": "docker compose up",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "smui-prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@smui/banner": "^8.0.3",
    "@smui/button": "^8.0.3",
    "@smui/card": "^8.0.3",
    "@smui/circular-progress": "^8.0.3",
    "@smui/list": "^8.0.3",
    "@smui/radio": "^8.0.3",
    "@smui/textfield": "^8.0.3",
    "@smui/top-app-bar": "^8.0.3",
    "@smui/touch-target": "^8.0.3",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/adapter-node": "^5.3.3",
    "@sveltejs/kit": "^2.43.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22",
    "@vitest/browser": "^3.2.4",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "playwright": "^1.55.1",
    "sass": "^1.93.2",
    "smui-theme": "^8.0.3",
    "svelte": "^5.39.5",
    "svelte-check": "^4.3.2",
    "svelte-jester": "^5.0.0",
    "svelte-preprocess": "^6.0.3",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "vitest-browser-svelte": "^1.1.0"
  },
  "dependencies": {
    "@smui-extra/bottom-app-bar": "^8.0.3",
    "@smui/dialog": "^8.0.3",
    "@smui/fab": "^8.0.3",
    "@smui/form-field": "^8.0.3",
    "@smui/menu": "^8.0.3",
    "@smui/switch": "^8.0.3",
    "@smui/tab": "^8.0.3",
    "@smui/tab-bar": "^8.0.3",
    "dotenv": "^17.2.3",
    "postgres": "^3.4.7",
    "svelte-time": "^2.0.2",
    "zod": "^4.1.12"
  }
}
